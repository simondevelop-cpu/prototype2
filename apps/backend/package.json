{
  "name": "backend",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.15.0",
    "bcryptjs": "2.4.3",
    "cors": "2.8.5",
    "cookie-parser": "1.4.6",
    "csv-parse": "5.5.6",
    "date-fns": "3.6.0",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "i18next": "23.10.1",
    "multer": "1.4.5-lts.1",
    "nanoid": "5.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/multer": "1.4.11",
    "@types/node": "20.12.7",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "7.8.0",
    "@typescript-eslint/parser": "7.8.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "prettier": "3.2.5",
    "prisma": "5.15.0",
    "supertest": "6.4.2",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "tsup": "8.0.2",
    "tsx": "4.7.1",
    "typescript": "5.4.5"
  }
}
